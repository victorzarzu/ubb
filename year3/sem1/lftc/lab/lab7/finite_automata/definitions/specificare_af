SpecificareAF = Tip"\n"Stare"\n"ListaStari"\n"ListaTranzitii"\n".

Tip = "ID"|"CONST".

Stare = CaracterInitial{Caracter}.
CaracterInitial = "a"|"b"|...|"z".
Caracter = CaracterInitial|"0"|"1"|...|"9".

ListaStari = {Stare}.

Tranzitie = Stare ListaRangeSimboluri Stare.
ListaRangeSimboluri = RangeSimboluri {"," RangeSimboluri}.
RangeSimboluri = Simbol | Simbol"-"Simbol.
Simbol = Caracter|"."|"{"|"}"|"("|")"|"=".
